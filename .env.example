# Server Configuration
PORT=3000
HOST=localhost
NODE_ENV=development

# JWT Authentication
JWT_SECRET=your-jwt-secret-key-here

# Database - PostgreSQL
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/magic_server_db?schema=public"

# Observability
OTLP_ENDPOINT=http://localhost:4317
SERVICE_NAME=magic-server

# Logging
LOG_LEVEL=info

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Transport (websocket/http)
DEFAULT_TRANSPORT=websocket
WEBSOCKET_PORT=8080
HTTP_PORT=8081